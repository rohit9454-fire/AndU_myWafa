{
  "name": "MyWafa",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 15 Pro'",
    "reset": "rm -rf node_modules && yarn install && cd ios && rm -rf Pods Podfile.lock build && pod install && cd ..",
    "ios:mywafaDevelopment": "ENVFILE=config/.env.mywafa_development react-native run-ios --scheme 'mywafa_development' --simulator='iPhone 16 Pro'",
    "ios:mywafaProduction": "ENVFILE=config/.env.mywafa_production react-native run-ios --scheme 'mywafa_production' --simulator='iPhone 16 Pro'",
    "android:mywafaDevelopment": "yarn android:clean && sh -c 'cp config/.env.mywafa_development .env' && react-native run-android --mode=mywafaDevelopmentDebug --appId=com.gmg.loyalty.dev",
    "android:mywafaProduction": "yarn android:clean && sh -c 'cp config/.env.mywafa_production .env' && react-native run-android --mode=mywafaProductionDebug --appId=com.gmg.loyalty.dev",
    "android:apk:mywafaDevelopment": "cd android && ENVFILE=config/.env.mywafa_development ./gradlew assembleDebug && cd ..",
    "android:apk:mywafaProduction": "cd android && ENVFILE=config/.env.mywafa_production ./gradlew assembleRelease && cd ..",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
